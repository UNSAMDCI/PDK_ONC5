# Scan FF for DFT Attributes

set tc "ff(IQ,IQN) { clocked_on : ICLK; next_state : D; } \
	pin(ICLK) { clock : true; direction:input; } \
	pin(SD) { direction:input; signal_type : test_scan_in; } \
	pin(SE) { direction:input; signal_type : test_scan_enable; } \
	pin(D) { direction:input; } \
	pin(Q) { direction: output; function : IQ; signal_type : test_scan_out; } \
	pin(QN) { direction: output; function : IQN; signal_type : test_scan_out_inverted; }" 
add_liberty_group -explicit -cell SDFRX1 test_cell "\"$tc\""